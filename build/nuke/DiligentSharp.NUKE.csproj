    <PackageReference Include="Silk.NET.BuildTools" Version="2.12.0" />
    <PackageReference Include="libclang" Version="13.0.1" />
    <PackageReference Include="Nuke.Common" Version="6.0.0" />
  </ItemGroup>

</Project>
